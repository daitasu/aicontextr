{
  "name": "@daitasu/aicontextr",
  "version": "0.0.1",
  "description": "AI context file generator for MDC and CLAUDE.md files",
  "main": "bin/index.js",
  "bin": {
    "aicontextr": "bin/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start": "node bin/index.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "pnpm run build"
